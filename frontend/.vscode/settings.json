{
  "editor.tabSize": 2,
  "python.linting.pylintEnabled": true,
  "python.linting.enabled": true,
  "cSpell.words": [
    "autosize",
    "baloo",
    "botocore",
    "box",
    "covid",
    "docgen",
    "dropzone",
    "geocode",
    "geolocate",
    "iphonex",
    "map",
    "map box",
    "mapboxgl",
    "rehydrated",
    "signup",
    "tfstate",
    "theming",
    "unmounts",
    "viewports"
  ],
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "[css]": {
    "editor.formatOnSave": false
  },
  "importSorter.importStringConfiguration.quoteMark": "double",
  "importSorter.importStringConfiguration.maximumNumberOfImportExpressionsPerLine.count": 88,
  "importSorter.importStringConfiguration.maximumNumberOfImportExpressionsPerLine.type": "newLineEachExpressionAfterCountLimit",
  "importSorter.importStringConfiguration.tabSize": 2
}
